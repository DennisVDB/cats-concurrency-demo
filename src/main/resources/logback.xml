<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%-5level [%thread]: %message%n</pattern>
        </encoder>
    </appender>

    <root level="${GENERAL_LOG_LEVEL}">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>

